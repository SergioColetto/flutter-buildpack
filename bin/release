#!/bin/sh

BUILD_DIR=${1:-}
# If the FLUTTER_CLEANUP is in false, or in true.
if [ -d $BUILD_DIR/build/web ]; then
    DEPLOY_DIR="/app/build/web/"
else
    DEPLOY_DIR="/app/"
fi

cat <<EOF
---
default_process_types:
    web: /app/.puro/envs/onboarding_waiter/flutter/bin/cache/dart-sdk/bin/dart pub global run dart_server_httpd

EOF
